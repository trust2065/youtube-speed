{
  "manifest_version": 3,
  "name": "YouTube Speed Controller",
  "version": "1.0",
  "description": "Easily control YouTube playback speed.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}